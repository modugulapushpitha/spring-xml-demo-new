<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="actor1" class="com.stackroute.spring.domain.Actor">
    <constructor-arg name="name" value="Yash"></constructor-arg>
        <constructor-arg name="age" value="40"></constructor-arg>
        <constructor-arg name="gender" value="male"></constructor-arg>
    </bean>
    <bean id="actor2" class="com.stackroute.spring.domain.Actor">
        <constructor-arg name="name" value="Yash"></constructor-arg>
        <constructor-arg name="age" value="40"></constructor-arg>
        <constructor-arg name="gender" value="male"></constructor-arg>
    </bean>
    <bean id="actor3" class="com.stackroute.spring.domain.Actor">
        <constructor-arg name="name" value="Yash"></constructor-arg>
        <constructor-arg name="age" value="40"></constructor-arg>
        <constructor-arg name="gender" value="male"></constructor-arg>
    </bean>
    <bean id="mobj1" class="com.stackroute.spring.domain.Movie">
        <constructor-arg name="mname" value="KGF"></constructor-arg>
        <constructor-arg name="aobj" ref="actor1"></constructor-arg>
    </bean>
    <bean id="mobj2" class="com.stackroute.spring.domain.Movie">
        <constructor-arg name="mname" value="KGF2"></constructor-arg>
        <constructor-arg name="aobj" ref="actor2"></constructor-arg>
    </bean>
    <bean id="mobj3" class="com.stackroute.spring.domain.Movie" scope="prototype">
        <constructor-arg name="mname" value="KGF"></constructor-arg>
        <constructor-arg name="aobj" ref="actor3"></constructor-arg>
    </bean>
</beans>