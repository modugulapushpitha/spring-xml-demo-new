<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="actor" class="com.stackroute.spring.domain.Actor">
        <property name="name" value="Yash"></property>
        <property name="age" value="40"></property>
        <property name="gender" value="male"></property>
    </bean>
    <bean id="mobj" class="com.stackroute.spring.domain.Movie">
        <property name="mname" value="KGF"></property>
    <property name="aobj" ref="actor"></property>
    </bean>
</beans>